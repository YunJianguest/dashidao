						//减
						function reduce(i,len){
							var values = $("#input-num"+i).val();
							if(values>0){
								$("#input-num"+i).attr("value",values-1);
								var price = $("#price"+i).html();
								$("#totalPrice"+i).html(price*(values-1));
							}else{
								$("#input-num"+i).attr("value",0);
							}
						}
						
						//加
						function add(i,len){
							var values = $("#input-num"+i).val();
							$("#input-num"+i).attr("value",(values*1+1));
							
							var price = $("#price"+i).html();
							$("#totalPrice"+i).html(price*(values*1+1));
							
							var total_price = 0;
							for(var j=0;j<len;j++){
