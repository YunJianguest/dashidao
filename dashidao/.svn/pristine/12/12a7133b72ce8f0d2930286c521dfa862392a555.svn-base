                        //定义函数
                        function tab(tabID,box){
                            $(tabID).each(function(){
                                $(this).click(function(){
                                    $(tabID).removeClass('active'); //先将所有同级去掉active
                                    $(this).addClass('active');   //当前li显示
                                    if($(tabID).html().indexOf("进货单")>=0){
                                        $(box).hide();   //所有同级box隐藏
                                        $(box).eq($(this).index()).show();
                                    }else if($(tabID).html().indexOf("下单进货")>=0){
                                    	window.location.href="$!webPath/poster/ordersave.htm";
                                    } else{
                                        var order_status=-1;
                                        var url=window.location.href;
                                        if( $(this).html()=="全部"){

                                        }else if($(this).html().indexOf("待付款")>=0){
                                            order_status="order_submit";

                                        }else if($(this).html().indexOf("待发货")>=0){
                                            order_status="order_tosendgoods";

                                        }else if($(this).html().indexOf("待收货")>=0){
                                            order_status="order_fotgoods";

                                        }
                                        if(url.indexOf('order_status')>=0){
                                            url=url.substring(0,url.indexOf('order_status')+12)+"="+order_status;
                                        }else{
                                            if(url.indexOf('?')>=0){
                                                url=url+"&order_status="+order_status;
                                            }else{
                                                url=url+"?order_status="+order_status;
                                            }

                                        }
                                        if(order_status==-1){
                                            url=url.replace("&order_status=-1","");
                                            url=url.replace("order_status=-1","");
                                        }
                                        window.location.href=url;
                                    }


                                });
                            });
                            $(tabID).removeClass('active');
                            $(tabID).each(function(){
                                if($(this).html().indexOf("进货单")>=0){
                                    $(this).addClass('active');
                                }
                                if('${order_status}'=="order_submit"&&$(this).html().indexOf("待付款")>=0){
                                    $(this).addClass('active');
                                }
                                if('${order_status}'=="order_tosendgoods"&&$(this).html().indexOf("待发货")>=0){
                                    $(this).addClass('active');
                                }
                                if('${order_status}'=="order_fotgoods"&&$(this).html().indexOf("待收货")>=0){
                                    $(this).addClass('active');
                                }
                                if('${order_status}'=="all"&&$(this).html().indexOf("全部")>=0){
                                    $(this).addClass('active');
                                }
                            });

                        }
                        
