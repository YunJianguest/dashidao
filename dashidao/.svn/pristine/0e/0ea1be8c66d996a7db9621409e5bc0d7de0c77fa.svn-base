e_size="+pagelimit+"&page_no="+cuur+"&moudle_id="+moudle_id,             error:function(){alert("数据错误，操作被取消！");},             success:function(data){            	             	 if(data.state=="error"){alert("系统错误，操作被取消！");return;}                 total=data.data.total;                 var message=data.data.data;                 $("#tableless").html('');                 var html ="";                 for(var i=0;i<message.length;i++)                 {                	  html += "<tr class='first'><td width='33%'><span class='fl'>"+message[i]['zd_shangpingname'];                	  html +=  "<br/><img src='$!webPath/resources/style/system/manage/black/img/11.png'/><font>"+message[i]['zd_shopcolor']+"</font></span>";                	  html += "<span class='fr'>商品编号: "+message[i]['zd_shangpingid']+"<br/>尺码:"+message[i]['zd_chima'];                	  html += "<br/>颜色："+message[i]['zd_shopcolor']+"</span></td>";                	  html +=  "<td width='17%'>"+message[i]['zd_chuchangjia']+"/"+message[i]['zd_lingshoudanwei']+"</td>";                	  html +=   "<td width='16%'>"+message[i]['zd_qidingshuliang']+"箱</td>";                	  html +=    "<td width='18%'>"+message[i]['zd_shelflife']+"<br/>保质期:"+message[i]['zd_shelfdate'];                	  html +=    " <td width='18%' class='last' style='border-bottom:1px solid #ccc;'>";                	  html +=  "<p> <span onclick='delShangPian(\""+message[i]['id']+"\")'>删除</span></p><p><span op='orde'><a href='$!webPath/poster/agentdetail.htm'>详情</a> </span></p></td>";                	  html += "</tr>";                 }                 $("#tableless").append(html);             }    	 });     }    </script></head><body><div class="main">    <div class="user_center">        $!httpInclude.include("/poster/nav_head.htm")        <table width="100%" border="0" cellspacing="0" cellpadding="0" class="user_table">            <tr> $!httpInclude.include("/poster/nav.htm?op=refund")                <td id="c