ul,ol li{list-style: none;}
ul,ol{margin: 0!important;}
*{margin: 0;padding: 0;}
a{text-decoration-style: none;}
.clear{clear: both;}
/*盈亏管理*/
.header_both{width:60%;margin: 0 20%;text-align: center;}
.header_both ul li{float:left;width:20%;height: 100px;line-height:98px;color:#fff;}
.header_both input{margin-left:5px;}
.yil{font-size: 20px;}
.money{font-size: 36px;}
.details span{font-size:14px;background: #fff;color:#000;padding: 5px 25px;border-radius: 5px;}
.header_both div{height: 50px;line-height: 50px;}.header_both div i{font-size: 20px;font-size:14px;}
.both>ul>li{width:49%;background:#C0E1F0 100%;border: 1px solid #BBBBBB 100%;float: left;border:1px solid #BBBBBB;height: 479px;}
.both{width:99%;margin:4px 1%;}.zhichu{margin-left:1%;}
.both>ul>li>div{width:100%;height: 29px;line-height: 29px;font-size: 20px;margin-top:10px;padding-left: 30%;}
.both>ul>li>ol{margin-top:10px;}
.both>ul>li>ol>li{font-size: 16px;height: 32px;line-height: 32px;margin-left:32%;}
.shouru{ position: relative;}
.xiang{width:80px!important;height: 30px!important;background: #fff;margin: 0 auto;border-radius: 5px;border:1px solid #BBBBBB!important;font-size: 14px!important;bottom:20px; left: 40%; position: absolute;text-align: center;line-height: 30px;color:#000;}
/*盈亏管理详情*/
.table_header{background:#DFE2F5;height: 39px;line-height: 20px;color: #13A0F0;}
.table_header ul li{width:16.5%;float:left;height: 39px;line-height:35px;text-align: center;}
.table_content ul li{width:16.5%;float:left;height: 50px;line-height:35px;text-align: center;}
/*商家对账单管理*/
.merchant_header{background:#DFE2F5;height: 39px;line-height: 20px;color: #13A0F0;}
.merchant_header ul li{width:12.5%;float:left;height: 39px;line-height:35px;text-align: center;}
.merchant_content ul li{width:12.5%;float:left;height:60px;line-height:60px;text-align: center;border-bottom: 1px solid #bbbbbb;}
.merchant_content ul li span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;}
/*商家对账明细*/
.business_header{height: 39px;line-height: 20px;color: #13A0F0;}
.business_header ul li{width:20%;float:left;height: 39px;line-height:35px;text-align: center;}
.business_content ul li{width:20%;float:left;height:60px;line-height:60px;text-align: center;}
.business_content ul li span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;}
.business_content span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;text-align: center;float: right;margin: 20px 10px;}
/*税金退税*/
.drawback_header{background:#DFE2F5;height: 39px;line-height: 20px;color: #13A0F0;}
.drawback_header ul li{width:12.5%;float:left;height: 39px;line-height:35px;text-align: center;}
.drawback_content ul li{width:12.5%;float:left;height:60px;line-height:60px;text-align: center;border-bottom: 1px solid #bbbbbb;}
.drawback_content ul li span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;margin-left: 5px;text-align: center;}
.operation{width:25%!important;text-align:left!important; }
/*税金退税详情*/
.derw_title{border-left:10px solid #108EE9;width:90%;margin: 0 5%;height: 31px;line-height: 31px;padding-left:10px;margin-top: 10px;font-size: 18px;}
.derw_title span{margin: 0 15px;}
.from ul li{float:left;font-size:14px;height: 35px;line-height: 35px;}
.draw_detail ul li{float:left;font-size:14px;height: 35px;line-height: 35px;}
.one{width:10%;}
hr{left: 270px;top: 238px;width: 707px;margin-top: 15px;}
.two{width:90%;}
.news ul li{float:left;font-size:14px;height: 35px;line-height: 35px;}
.news>div>ul>.one{width:18%!important;}
.news>div>ul>.two{width:15%!important;}
.news>div>ul>.three{width:60%!important;}
.invoice i{font-size:20px;}
.on{background:#5677fc;color:#fff;border-radius: 50%;}
.invoice .invo_stuta{height: 50px;line-height: 35px; }
.invoice .invo_stuta span{margin:20px 20px;}
.remarks span{float: left;margin-left: 20px;}
.sub{background:#108EE9;height: 49px;width:268px;margin:80px 0 0 25%;line-height:49px;border-radius:30px;text-align: center;color:#fff;font-size: 16px;}
/*分页*/
#page li{cursor:pointer;}
/*商品警察*/
.spjc_header{background:#DFE2F5;height: 39px;line-height: 20px;color: #13A0F0;}
.spjc_header ul li{width:10.5%;float:left;height: 39px;line-height:35px;text-align: center;}
.spjc_content ul li{width:10.5%;float:left;height:60px;line-height:60px;text-align: center;}
.spjc_content ul li span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;margin-left: 5px;text-align: center;}
/*法务监督*/
.fwjd_header{background:#DFE2F5;height: 39px;line-height: 20px;color: #13A0F0;width:94%;margin:0 3%;}
.fwjd_header ul li{width:12.5%;float:left;height: 39px;line-height:35px;text-align: center;}
.fwjd_content{width:94%;margin:0 3%;}
.fwjd_content ul li{width:12.5%;float:left;height:60px;line-height:60px;text-align: center;border-radius: 5px;}
.fwjd_content ul{border: 1px solid #bbbbbb;border-radius: 5px; margin-top:5px!important;}
.fwjd_content ul li span{width:85px;height: 26px;line-height:26px;border:1px solid #BBBBBB;display: inline-block;border-radius:5px;margin-left: 5px;text-align: center;}
.fwjd_operation{width:25%!important;text-align:left!important; }
.fwjd_operation a{color:#000;}
.fwsousu{width:94%;margin:10px 3%;background: #DCEFFA;height: 50px;line-height: 45px;}
.fwsousu input{width:40%;height:30px; line-height: 30px;border-radius: 5px;border: 1px solid #c3c3c3;margin: 0 2%;}
.sousu{margin: 10px 0;}
.sousu input{width:80%;height:30px; line-height: 30px;border-radius: 5px;border: 1px solid #c3c3c3;margin: 0 2%;}
.fwjdcz{float:right;}.fwjdcz i{margin: 0 5px;}
/*日志管理删除*/
.rztable td{height: 50px;line-height: 50px;border-bottom: 1px dashed #ccc;}
.rztable span{border:1px solid #0A78FA;border-radius: 5px;padding:6px 10px;}
.four_xi{color:#F15755;}.three_xi{color:#FF4081;}.two_xi{color:#0A78FA;}
/*消费者保障服务*/
.bzcz span{background: #108EE9;padding: 5px 20px;color: #fff;margin-right: 20px;float: right;border-radius: 5px;}	
.tle{font-size: 17px!important;color:#000!important;}
.bz{font-size: 15px;}
.bz table td{height: 110px;}
.bz table td div{line-height: 26px;color:#555555;}
.bzj{color:#000!important;font-size: 14px;}
.bz table td{
    height: 30px;line-height: 30px;
}