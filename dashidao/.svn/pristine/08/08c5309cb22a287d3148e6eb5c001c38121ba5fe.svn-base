							<script>
                                layui.use(['laypage', 'layer'], function(){
                                    var laypage = layui.laypage
                                        ,layer = layui.layer;

                                    //总页数低于页码总数
                                    laypage.render({
                                        elem: 'demo0'
                                        ,count: 50 //数据总数
                                    });

                                    //总页数大于页码总数
