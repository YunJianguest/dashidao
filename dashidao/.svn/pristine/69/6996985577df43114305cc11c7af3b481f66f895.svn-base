                                        page_size=obj.limit;
                                        //alert(pagelimit);
                                        zhandai_analy(page_size,page_no);
                                        //loadajax("select","stu",pagelimit,cuur,1);
                                    },
                                    limits:[2, 4, 6, 8, 10],
                                    limit:page_size
                                });
                            });
                            goods_analy(page_size,page_no);
                            layui.use(['laypage', 'layer'], function(){
                                var laypage = layui.laypage
                                    ,layer = layui.layer;
                                //完整功能
                                laypage.render({
                                    elem: 'demo7'
                                    ,count:count1
                                    ,layout: ['count', 'prev', 'page', 'next', 'limit', 'skip']
                                    ,jump: function(obj){
                                        //console.log(obj)
                                        page_no=obj.curr;
                                        page_size=obj.limit;
                                        //alert(pagelimit);
                                        goods_analy(page_size,page_no);
                                        //loadajax("select","stu",pagelimit,cuur,1);
                                    },
                                    limits:[2, 4, 6, 8, 10],
                                    limit:page_size
                                });
                            });
                        });
                        function zhandai_analy(page_size,page_no){
                            var sql = "SELECT dashidao_goodscart.`goodsname`,dashidao_goodscart.`price`,dashidao_goodscart.`count`,dashidao_goodscart.`spec_info`,ZHANDAI.userName ,dashidao_goods.`goods_serial` " +
                                "FROM dashidao_goodscart " +
                                "LEFT JOIN( " +
                                "SELECT dashidao_user.`userName`,dashidao_orderform.`id`,dashidao_orderform.`seller` " +
                                "FROM dashidao_orderform  INNER JOIN dashidao_user  ON dashidao_orderform.`seller`=dashidao_user.`id` " +
                                "WHERE dashidao_user.`userRole`='POSTER' " +
                                ")ZHANDAI ON ZHANDAI.id=dashidao_goodscart.`of_id` " +
                                "LEFT JOIN dashidao_goods ON dashidao_goodscart.`goods_id`=dashidao_goods.`id`"+
                                "WHERE ZHANDAI.`seller`="+userId+" order by dashidao_goodscart.id ";
                            jQuery.ajax({
                                type: "POST",
                                cache: false,
                                async: false,
                                url: "/ajaxuser.jsp",
                                dataType: "json",
                                data: "table=dashidao_goodscart&dates=addTime&sql="+sql+"&page_size="+page_size+"&page_no="+page_no,
                                error: function () {
                                    alert("数据错误，操作被取消！");
                                },
                                success: function (data) {
                                    if (data.state == "error") {
                                        alert("系统错误，操作被取消！");
                                        return;
                                    }
                                    count1=data.data.total;
                                    var message = data.data.data;
                                    $("#goods_zhandai").html('');
                                    for (var i = 0; i < message.length; i++) {
                                        var i1=i+1;
                                        $("#goods_zhandai").append("<tr>" +
                                            "<td>" + i1 + "</td>" +
                                            "<td>" + message[i]['userName'] + "</td>" +
                                            "<td>" + message[i]['goods_serial'] + "</td>" +
                                            "<td>" + message[i]['goodsname'] + "</td>" +
                                            "<td>" + message[i]['spec_info'] + "</td>" +
                                            "<td>" + message[i]['price'] + "</td>" +
                                            "<td>" + message[i]['count'] + "</td>" +
                                            "<td>" +parseFloat(message[i]['price'])*parseFloat(message[i]['count'])+ "</td></tr>" );
                                    }
                                }
                            });
                        }
                        function goods_analy(page_size,page_no){
                            var sql = "SELECT dashidao_goodscart.`goodsname`,dashidao_goodscart.`price`,dashidao_goodscart.`count`,dashidao_goodscart.`spec_info`,dashidao_goods.`goods_serial` " +
                                "FROM dashidao_goodscart  " +
                                "LEFT JOIN dashidao_orderform ON dashidao_orderform.id=dashidao_goodscart.`of_id` " +
                                "LEFT JOIN dashidao_goods ON dashidao_goodscart.`goods_id`=dashidao_goods.`id` " +
                                "WHERE dashidao_orderform.`seller`="+userId+" order by dashidao_goodscart.id ";
                            jQuery.ajax({
                                type: "POST",
                                cache: false,
                                async: false,
                                url: "/ajaxuser.jsp",
                                dataType: "json",
                                data: "table=dashidao_goodscart&dates=addTime&sql="+sql+"&page_size="+page_size+"&page_no="+page_no,
                                error: function () {
                                    alert("数据错误，操作被取消！");
                                },
                                success: function (data) {
                                    if (data.state == "error") {
                                        alert("系统错误，操作被取消！");
                                        return;
                                    }
                                    count1=data.data.total;
                                    var message = data.data.data;
                                    $("#tablesss").html('');
                                    for (var i = 0; i < message.length; i++) {
                                        var i1=i+1;
                                        $("#tablesss").append("<tr>" +
                                            "<td>" + i1 + "</td>" +
                                            "<td>" + message[i]['goods_serial'] + "</td>" +
                                            "<td>" + message[i]['goodsname'] + "</td>" +
                                            "<td>" + message[i]['spec_info'] + "</td>" +
                                            "<td>" + message[i]['price'] + "</td>" +
                                            "<td>" + message[i]['count'] + "</td>" +
                                            "<td>" +parseFloat(message[i]['price'])*parseFloat(message[i]['count'])+ "</td></tr>" );
                                    }
                                }
                            });
                        }
