        function loadajax(page_size,page_no){
            var sql="SELECT * FROM zd_yijianqiuzhu ORDER BY yj_beginTime DESC";
            jQuery.ajax({
                type:"POST",
                cache: false,
                async:false,
                url:"/ajaxuser.jsp",
                dataType:"json",
                data:"table=zd_yijianqiuzhu&dates=yj_beginTime,yj_endTime,zd_time,addTime&sql="+sql,
                //data:"table="+table+"&field="+field+"&values="+values+"&page_size="+page_size+"&page_no="+page_no+"&moudle_id="+moudle_id,
                error:function(){alert("数据错误，操作被取消！");},
                success:function(data)
                {
                    if(data.state=="error"){alert("系统错误，操作被取消！");return;}
                    var message=data.data.data;
                    count1=data.data.total;
                    $("#tablesss").html('');
                    for(var i=0;i<message.length;i++){
                        var beginTime = (new Date(message[i]['yj_beginTime'])).getTime(); //得到开始毫秒数
                        var endTime = (new Date(message[i]['yj_endTime'])).getTime(); //得到结束毫秒数
                        var ne=(endTime-beginTime)/1000/60/60;
                        var sts=message[i]['states'];
                        var id = message[i]['id'];
                        var stt;
                        if(sts==0){stt='已提交'}else if(sts==1){stt='协商中'}else if(sts==2){stt='已完结'}else if(sts==3){stt='维修中'}
                        $("#tablesss").append("<tr><td>"+message[i]['yj_orderNum']+"</td>" +
                            "<td>"+message[i]['yj_userId']+"</td>" +
                            "<td>"+message[i]['yj_servicePeople']+"</td>" +
                            "<td>"+message[i]['yj_serviceStyle']+"</td>" +
                            "<td>"+message[i]['yj_beginTime']+"</td>" +
                            "<td>"+stt+"</td>" +
                            "<td>"+message[i]['yj_solveTime']+"</td>" +
                            "<td>"+ne.toFixed(0)+"小时</td>" +
                            "<td>"
                            +"<span><a href='$!webPath/seller/sjseekhelp.htm?id="+message[i]['tc_tsid']+"&ddid="+message[i]['yj_orderNum']+"'>详情</a></span>"
                            +"<span><a href='#' onclick='update_status("+id+")'>修改</a></span>"
                            +"</td>"
                            +"</tr>");
                    }

                }
            });
        }
