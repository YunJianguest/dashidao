                                                    success: function (data) {
                                                        if(data.code=="0"){alert("系统错误，操作被取消！");return;}
                                                        if(data.msg=="操作成功"){

                                                        }
                                                    }
                                                });
                                            }

                                            //alert(data.data);
                                        }

                                    });

