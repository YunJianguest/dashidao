                                    },
                                    success: function (data) {
                                        if (data.state == "error") {
                                            alert("系统错误，操作被取消！");
                                            return;
                                        }
                                       var mess=data.data.data;
                                       var str=mess[0]['yk_tjsj_yj'];
                                       tex=parseFloat(str.split(",")[1]);
                                       
                                      /*  var huiyuan=$(this).closest(".first").find("#huiyuan");
                                       var yunke=$(this).closest(".first").find("#yunke");
