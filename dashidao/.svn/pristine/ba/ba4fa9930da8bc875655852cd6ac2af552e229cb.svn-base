<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><title>代理商品 - $!{config.poweredby}</title><meta name="keywords" content="$!config.keywords" /><meta name="description" content="$!config.description" /><meta name="generator" content="$!{config.meta_generator}" /><meta name="author" content="$!{config.meta_author}"><meta name="copyright" content="$!{config.copyRight}">#if($!config.website_ico)<link rel="shortcut icon"	href="$!imageWebServer/$!config.website_ico.path/$!config.website_ico.name"	type="image/x-icon" />#end<link	href="$!webPath/resources/style/system/front/default/css/iconfont.css"	type="text/css" rel="stylesheet" /><link href="$!webPath/resources/style/system/front/default/css/mcss.css"	type="text/css" rel="stylesheet" /><link	href="$!webPath/resources/style/system/front/default/css/public.css"	type="text/css" rel="stylesheet" /><link href="$!webPath/resources/style/system/front/default/css/user.css"	type="text/css" rel="stylesheet" /><link href="$!webPath/resources/style/common/css/overlay.css"	type="text/css" rel="stylesheet" /><link	href="$!webPath/resources/style/common/css/jquery-ui-1.8.22.custom.css"	type=text/css rel=stylesheet><script src="$!webPath/resources/js/jquery-1.8.3.min.js"></script><script src="$!webPath/resources/js/jquery-ui-1.8.21.js"></script><script src="$!webPath/resources/js/jquery.zh.cn.js"></script><script src="$!webPath/resources/js/jquery.shop.common.js"></script><!--bootstrap--><link rel="stylesheet"	href="$!webPath/resources/style/system/front/default/css/bootstrap.min.css"	type="text/css" rel="stylesheet"><link rel="stylesheet"	href="$!webPath/resources/style/system/front/default/css/bootstrap-theme.min.css"	type="text/css" rel="stylesheet"><script	src="$!webPath/resources/style/system/front/default/js/bootstrap.min.js"></script><!--layui--><link rel="stylesheet"	href="$!webPath/resources/style/system/front/default/css/layui.css"	type="text/css" rel="stylesheet"><script src="$!webPath/resources/style/system/front/default/layui.js"></script><script type="text/javascript"> 	 	function  zddel(id){ 		var data={				id:id				}		jQuery.ajax({		    type:"POST",		    cache: false,		    async:false,		    url:"$!webPath/poster/agentcommodity_del.htm",		    dataType:"json",		    data:data,		    error:function(){},		    success:function(data)		    { 		    	alert(data.data);		    	window.location.href=window.location.href;		    }		});	} 	 	</script></head><body>	<div class="main">		<div class="user_center">			$!httpInclude.include("/poster/nav_head.htm")			<table width="100%" border="0" cellspacing="0" cellpadding="0"				class="user_table">				<tr>					$!httpInclude.include("/poster/nav.htm?op=refund")					<td id="centerbg" valign="top" width="80%">						<div class="buyer_position" style="width: 99%; background: #fff;">							<div class="buyer_p_box">								<a href="$!webPath/poster/index.htm">栈代支持</a> > <span>商品管理</span>								> <span>代理商品</span>							</div>						</div>						<div class="shopadminbox choicebox">							<table width="100%" border="0" cellspacing="0" cellpadding="0">								<tbody>																		<link										href="$!webPath/resources/style/system/manage/blue/tip2.css"										rel="stylesheet" type="text/css" />									<tr class="tiptitles">										<td width="100%" colspan="7">											<div class="operation_tip" style="margin-bottom: 30px;">												<h3>友情提示</h3>												<ul>													<li></li>													<li></li>												</ul>											</div>										</td>									</tr>																		<tr class="returnthead">										<td width="100%" colspan="7">											<form class="form-inline" style="float: right;">												<div class="form-group">													<select id="sel" onchange="selChange()" style="width:200px;">														<option value="">请选择商家</option>														#foreach($obj in $objs)														<option value="$!obj.store.id">$!obj.store.store_name</option>														#end																											</select>												</div>																							</form>										</td>									</tr>									</tbody>							</table>														<table width="100%" border="0" cellspacing="0" cellpadding="0">								<tbody id="table2">									<tr id="thead"> 										<td width="15%">商品名称</td>										<td align="center">操作</td>									</tr>																																			</tbody>							</table>							<!--分页开始-->                                    <div id="demo1" style=" text-align: center; margin-top: 30px;" ></div>                                    <script>                                        layui.use('laypage', function(){                                            var laypage = layui.laypage;                                            laypage.render({                                                elem: 'demo1' //注意，这里的 test1 是 ID，不用加 # 号                                                ,count: '${rows}' // 数据总数，从服务端得到                                                ,curr:'${currentPage}' // 服务器端回传当前页                                                ,jump: function(obj, first){                                                    //obj包含了当前分页的所有参数，比如：                                                    console.log(obj.curr);  //得到当前页，以便向服务端请求对应页的数据。                                                    console.log(obj.limit); //得到每页显示的条数                                                    //首次不执行,使用原始的curr,后面需要自己通过回传来更新                                                    var curr = obj.curr;                                                    if(!first){                                                        var url = window.location.href;                                                        if(url.indexOf("?") >= 0){                                                            url = url.split("?")[0] + "?currentPage=" + obj.curr;//切割字符串的方法                                                        }else{                                                            url = url + "?currentPage=" + obj.curr;                                                        }                                                        location.href=url;                                                    }                                                }                                            });                                        });                                    </script>                                    <!--分页结束--> 			</table>					</div>		$!httpInclude.include("/footer.htm")	</div>	<script>	function selChange(){	     var data = $("#sel").val();	     console.log(data);	     $("#table2").html("");	     if(data==null||data==""){//未选择商家	    	 $("#table2").append(""+	    			 "<tr id='thead'>"+	    			 	"<td width='15%'>商品名称</td>"+	    				"<td align='center'>操作</td>"+	    			 "</tr>");	     }else{	    	 var sql="SELECT * FROM dashidao_goods WHERE deleteStatus=0 and goods_store_id="+data+" ORDER BY addTime ";	    	 $("#table2").append(""+	    			 "<tr id='thead'>"+	    			 	"<td width='15%'>商品名称</td>"+	    				"<td align='center'>操作</td>"+	    			 "</tr>");	    	    jQuery.ajax({	    	        type:"POST",	    	        cache: false,	    	        async:false,	    	        url:"/ajaxuser.jsp",	    	        dataType:"json",	    	        data:"table=dashidao_goods&dates=addTime&sql="+sql,	    	        error:function(){alert("数据错误，操作被取消！");},	    	        success:function(data){	    	        	var message = data.data.data;	    	            console.log(message);	    	            for(var i=0;i<message.length;i++){	    	            	var goods_name = message[i].goods_name;	    	            		    	            	 $("#table2").append(""+	    	    	    			 "<tr align='center'>"+	    	    	    			 	"<td width='15%'>"+goods_name+"</td>"+	    	    	    				"<td class='ac8' align='center'>"+	    	    	    				"<a href='javascript' >查看</a>&nbsp;"+										"<a href='javascript:zddel('$!obj.id')'>删除</a>"+										"</td>"+	    	    	    			 "</tr>");	    	            }	    	        }	    	    });	     }	 }	</script> </body></html> 