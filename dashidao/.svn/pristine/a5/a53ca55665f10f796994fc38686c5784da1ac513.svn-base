            		//$("#zd_shelfdate").val(message[0]['zd_shelfdate']);
            		var begin = message[0]['zd_shelfdate'];
            		begin = begin.substring(0,10);
            		end = end.substring(0,10);
            		$("#zd_shelfdate").html(begin);
                }
            }); 
        }
        
