        function shangPingByid()
        {
        	var id = "${id}";
    		var sql ="select * from zd_xuanzeshangping where id='"+id+"' order by zd_time  ";
        	//data参数
        	var dataStr = "table=zd_xuanzeshangping&";
        	    dataStr +="dates=zd_shelfdate,zd_time&";
