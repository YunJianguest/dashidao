/* CSS Document */
#font1{color:darkgreen}
#font2{color:red}
.font3{color:blue}
ul li{list-style:none;}
.table{width: 800px;align-content: center;margin: 15px auto}
.table_thead{background-color: #dfe2f5;;color: blue;}
.solid {border-bottom-style:solid;}
.button{background-color: #108ee9;color: white;width: 80px;height: 40px;font-size:16px;}
/*对账中心的button按钮*/
.dzzx_button{background-color: white;color: black;}
/*搜索框样式*/
.i_search{position: relative;top: 24px;left: 10px}
.i_search_input{padding-left: 30px}
.i_angle_left{font-size: 28px;}
.i_pencel_remove{position: relative;top:50px;}

/*歇业审核*/
.div_1{width: 800px;margin: 0 auto}
.div_2{width: 800px;display: flex;margin: 15px auto}
/*歇业申请详情*/
.xy_td{border-bottom:1px solid #959595; padding: 10px 0;
		width: 500px;
		height: 20px;}
/*按钮的table边框*/
.xy_table{width: 800px; margin-top: 20px;}
/*平台收费 资金管理上层背景图*/
.bg_div{width: 100%;height: 150px; display: flex;margin: 15px auto;border:1px solid #C7C7C7; background-color:#3C77C7 ;color: white;}
/*佣金退税审核详情*/
.little_title{width: auto;height: 10px;font-size: 20px;}
.yj_table{width: 800px; margin-top: 20px;margin-left: 20px;}
/*平台和资金管理，上层字体样式*/
.p_font{padding-left: 50px;padding-top: 50px;}
.s_font{font-size: 26px;}
/*资金管理，下层背景图样式*/
.bg_div2{width: 800px;height: 550px; display: flex;margin: 15px auto;background-color:#74B8C3 ;color: black;}
.bg_div3{width: 49.99%;border:1px solid #C7C7C7;}
/*资金管理，下层字体样式*/
.zj_little_font{padding-left:100px;}
.zj_button{background-color: white;color: black;}
/*供货商管理下层，开设直营店下层div*/
.ghs_div{width: 800px; display: flex;margin: 15px auto;border:1px solid #C7C7C7;}
/*货物调拨div*/
.hwdb_div1{width: 800px;height:40px;display: flex;margin: 15px auto;border:1px solid #C7C7C7; background-color: #119896;}
.hwdb_div1_p1{padding-top: 10px;padding-left: 10px;}
/*栈代动态*/
.zddt_div1{width: 900px;height: 180px; margin: 15px auto;border:1px solid #C7C7C7}
.zddt_div2{width: 800px;height: 75px; display: flex;margin: 15px auto;border:none}
.zddt_picture{padding-left: 10px;padding-top: 15px;}
.zddt_p{width: 800px; display: flex; margin-left: 650px}
   /*栈代动态中的直线*/
.zddt_line{margin-top: 100px;
    width: 900px;
    border-bottom: 1px solid #000000;width: 800px;display: flex;margin: 15px auto;}

