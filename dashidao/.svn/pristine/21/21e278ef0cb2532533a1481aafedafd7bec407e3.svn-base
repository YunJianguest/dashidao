                                            elem: 'demo3'
                                            ,count: 100
                                            ,first: '首页'
                                            ,last: '尾页'
                                            ,prev: '<em>←</em>'
                                            ,next: '<em>→</em>'
                                        });

                                        //不显示首页尾页
