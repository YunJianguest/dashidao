                                                    '<td>'+rec_maxStock+'</td>' +
                                                    '<td>'+message[i]['rec_maxAmount']+'</td>' +
                                                    '<td>'+message[i]['rec_maxCount']+'</td>' +
                                                    '<td>'+message[i]['rec_zmNum']+'</td>' +
