<script type="text/javascript">
$('.view span').click(function(){
	$(this).addClass('check');
	$(this).siblings('span').removeClass('check');
})
function subc(){ 
	if($('#shyj').val()==""){
		alert('请输入审核意见');
		return;
	}
	var state;
	if($('.refuse').hasClass('check')){
		state="不同意";
		
	}else{
		state="同意";
	}
	
	   jQuery.ajax({
