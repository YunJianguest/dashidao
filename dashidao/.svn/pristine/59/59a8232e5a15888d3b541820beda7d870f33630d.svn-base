<script>
    function sub(){

        var check="";
         $("input[name='xbTy']").each(function(){
             if(this.checked==true){
                 check=this.value;
			 }
		 });
         if(check==""){
             alert("请选择同意或者不同意");
             return;
		 }
        jQuery.ajax({
            type: "POST",
            cache: false,
            async: false,
            url: "/ajax.jsp",
            dataType: "json",
            data: "action=update&table=zb_sjxiaobaoshenhe&field=id,states&values=" + $("#shenheId").val()+"|"+check,
            error: function () {
                alert("数据错误，操作被取消2！");
            },
            success: function (data) {
                if (data.state == "error") {
                    alert("系统错误，操作被取消！");
                    return;
                } else {
                    alert("操作成功！");
                    location.href="$!webPath/admin/sell_consume_check.htm";
                }
            }
        });
		alert(check);
    }
    jQuery(document).ready(function() {
        var id = "$!id";
        // alert(id);
        var shenheId = "$!shenheId";
        // alert(shenheId);    id,states

        jQuery.ajax({
            type: "POST",
            cache: false,
            async: false,
            url: "/ajax.jsp",
            dataType: "json",
            data: "action=select&table=dsdsj&field=id&values=" + shenheId,
            error: function () {
                alert("数据错误，操作被取消2！");
            },
            success: function (data) {
                if (data.state == "error") {
                    alert("系统错误，操作被取消2！");
                    return;
                }
                var message = data.data.data;
                for (var i = 0; i < message.length; i++) {
                     $("#form1").find("tr").eq(0).find("td").eq(1).append(message[i]['businessid']);
                    $("#form1").find("tr").eq(1).find("td").eq(1).append(message[i]['name']);


                }
            }
        });
    })

</script>
