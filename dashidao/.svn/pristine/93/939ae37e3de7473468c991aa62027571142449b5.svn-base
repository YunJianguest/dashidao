    <script>
        var userId='${user.id}';
        //分页
        var page_size=10;
        var page_no=1;
        var count1=0;
        jQuery(document).ready(function(){
            pickup_card(page_size,page_no);
            layui.use(['laypage', 'layer'], function(){
                var laypage = layui.laypage
                    ,layer = layui.layer;
                //完整功能
                laypage.render({
                    elem: 'demo'
                    ,count:count1
                    ,layout: ['count', 'prev', 'page', 'next', 'limit', 'skip']
                    ,jump: function(obj){
                        //console.log(obj)
                        page_no=obj.curr;
                        page_size=obj.limit;
                        //alert(pagelimit);
                        pickup_card(page_size,page_no);
                        //loadajax("select","stu",pagelimit,cuur,1);
                    },
                    limits:[2, 4, 6, 8, 10],
                    limit:page_size
                });
            });
        });
        function pickup_card(page_size,page_no){
            var sql="SELECT * FROM dsdsj_pickup_card_manager where user_id="+userId+" order by id";
            jQuery.ajax({
                type: "POST",
                cache: false,
                async: false,
                url: "/ajaxuser.jsp",
                dataType: "json",
                data: "table=dsdsj_pickup_card_manager&dates=create_date1,create_date2&sql="+sql+"&page_size="+page_size+"&page_no="+page_no,
                error: function () {
                    alert("数据错误，操作被取消！");
                },
                success: function (data) {
                    if (data.state == "error") {
                        alert("系统错误，操作被取消！");
                        return;
                    }
                    count1=data.data.total;
                    var message = data.data.data;
                    $("#pick_up").html('');
                    for (var i = 0; i < message.length; i++) {
                        var i1=i+1;
                        $("#pick_up").append("<tr>" +
                            "<td>" + i1 + "</td>" +
                            "<td>" + message[i]['goods_number'] + "</td>" +
                            "<td>" + message[i]['goods_name'] + "</td>" +
                            "<td>" + message[i]['card_number'] + "</td>" +
                            "<td>" + message[i]['count'] + "</td>" +
                            "<td>" + message[i]['less'] + "</td>" +
                            "<td>" +message[i]['less']+ "</td></tr>" );
                    }
                }
            });
        }
    </script>
